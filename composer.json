{
  "name": "manuirrazabal/deskpro-api-client-php",
  "description": "PHP client for use with the Deskpro API. Forked from deskpro/deskpro-api-client-php",
  "type": "library",
  "config": {
    "platform": {
      "php": "8.0"
    }
  },
  "keywords": [
    "deskpro",
    "api"
  ],
  "homepage": "https://github.com/manuirrazabal/deskpro-api-client-php",
  "license": "BSD-3-Clause",
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.3",
    "psr/log": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "Deskpro\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  }
}
